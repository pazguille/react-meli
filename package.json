{
  "name": "react-meli",
  "version": "1.0.0",
  "description": "A React mini-app to search for products on MercadoLibre.",
  "scripts": {
    "server": "node_modules/.bin/serve -p 3002",
    "build:watch": "NODE_ENV=develop node_modules/.bin/watchify -t [ babelify ] src/js/index.js -o build/bundle.js -d",
    "build": "NODE_ENV=production node_modules/.bin/browserify -t [ babelify ] src/js/index.js -o build/bundle.js",
    "lint": "node_modules/.bin/eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pazguille/react-meli.git"
  },
  "author": "Guille Paz <guille87paz@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pazguille/react-meli/issues"
  },
  "homepage": "https://github.com/pazguille/react-meli#readme",
  "devDependencies": {
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.8.0",
    "serve": "^6.5.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
