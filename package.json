{
  "name": "react-meli",
  "version": "1.0.0",
  "description": "A React mini-app to search for products on MercadoLibre.",
  "scripts": {
    "server": "NODE_PATH=./server NODE_ENV=development node server/lib/server/index.js",
    "server:prod": "NODE_PATH=./server NODE_ENV=production node server/lib/server/index.js",
    "build:watch": "NODE_ENV=develop node_modules/.bin/watchify -t [ babelify ] client/src/js/index.js -o public/js/bundle.js -d",
    "build": "NODE_ENV=production node_modules/.bin/browserify -t [ babelify ] client/src/js/index.js -o public/js/bundle.js",
    "lint": "node_modules/.bin/eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pazguille/react-meli.git"
  },
  "author": "Guille Paz <guille87paz@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pazguille/react-meli/issues"
  },
  "homepage": "https://github.com/pazguille/react-meli#readme",
  "devDependencies": {
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babel-register": "^6.11.6",
    "express": "4.14.0",
    "node-fetch": "^1.6.0",
    "pug": "^2.0.0-beta4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
